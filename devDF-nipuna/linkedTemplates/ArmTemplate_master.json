{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "devDF-nipuna"
		},
		"LS_source_dev_sqlsever_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_source_dev_sqlsever'"
		},
		"LS_source_dev_sqlsever_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_source_dev_sqlsever'"
		},
		"ls_dev_nipuna_adlsgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_dev_nipuna_adlsgen2'"
		},
		"ls_synapse_target_sqldw_nipuna_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_synapse_target_sqldw_nipuna'"
		},
		"LS_source_dev_sqlsever_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "molli666"
		},
		"ls_dev_nipuna_adlsgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsgen2devnipuna.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "devDF-nipuna_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_dev_nipuna_adlsgen2_accountKey": {
						"value": "[parameters('ls_dev_nipuna_adlsgen2_accountKey')]"
					},
					"ls_synapse_target_sqldw_nipuna_connectionString": {
						"value": "[parameters('ls_synapse_target_sqldw_nipuna_connectionString')]"
					},
					"LS_source_dev_sqlsever_connectionString": {
						"value": "[parameters('LS_source_dev_sqlsever_connectionString')]"
					},
					"LS_source_dev_sqlsever_password": {
						"value": "[parameters('LS_source_dev_sqlsever_password')]"
					},
					"ls_dev_nipuna_adlsgen2_properties_typeProperties_url": {
						"value": "[parameters('ls_dev_nipuna_adlsgen2_properties_typeProperties_url')]"
					},
					"LS_source_dev_sqlsever_properties_typeProperties_userName": {
						"value": "[parameters('LS_source_dev_sqlsever_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}